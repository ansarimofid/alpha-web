{
  "env": {
    "browser": true,
    "jest": true
  },
  "extends": [
    "react-app",
    "airbnb"
  ],
  "rules": {
    "no-console": "off",
    "jsx-a11y/anchor-is-valid": [ "error", { "components": [ "Link" ], "specialLink": [ "to" ] } ],
